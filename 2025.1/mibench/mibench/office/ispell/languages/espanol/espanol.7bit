texchars        ()\[]{}<\>\\$*.%

allaffixes on

# defstringtype "list" ".list"
defstringtype "nroff" "nroff" ".mm" ".ms" ".me" ".man"

wordchars a	A
stringchar \'a	\'A
wordchars [b-e] [B-E]
stringchar \'e	\'E
wordchars [f-i] [F-I]
stringchar \'i	\'I
wordchars [j-n] [J-N]
stringchar \'n	\'N
stringchar o	O
stringchar \'o	\'O
wordchars [p-u] [P-U]
stringchar \'u	\'U
stringchar \"u	\"U
wordchars [v-z] [V-Z]

