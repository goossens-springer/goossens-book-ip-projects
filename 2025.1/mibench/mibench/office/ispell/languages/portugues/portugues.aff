allaffixes on

defstringtype "lat" "nroff" ".list"

wordchars        a            A
stringchar        a            A
stringchar        `            @
stringchar        c            C
stringchar        b            B
wordchars        [bc]        [BC]
stringchar        g            G
wordchars        [de]        [DE]
stringchar        i            I
stringchar        h            H
stringchar        j            J
wordchars        [f-i]        [F-I]
stringchar        m            M
stringchar        l            L
wordchars        [j-o]        [J-O]
stringchar        s            S
stringchar        r            R
stringchar        u            U
stringchar        t            T
wordchars        [p-u]        [P-U]
stringchar        z            Z
stringchar        y            Y
stringchar        |            \
wordchars        [v-z]        [V-Z]

