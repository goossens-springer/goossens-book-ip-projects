prefixes

flag *A:
    .       >      ANTI       # "FSEM=anti"       # ex. venenoso -> antivenenoso
flag *B:
    .       >      ANTI\-     # "FSEM=anti"       # ex. semita -> anti-semita

flag *S:
[^H]        >      DES        # "FSEM=not"       # ex. amarrar -> desamarrar
H           >    -H,DES       # "FSEM=not"       # ex. habitar -> desabitar

flag *I:
[^PLMR]     >      IN         # "FSEM=not"    # ex. finito -> infinito
P           >      IM         # "FSEM=not"    # ex. paciente->impaciente
[LM]        >      I          # "FSEM=not"    # ex. material->imaterial
R           >      IR         # "FSEM=not"    # ex. real->irreal

flag *R:
[^S]        >      RE         # "FSEM=outra"     # ex. encontrar -> reencontrar
S           >      RES        # "FSEM=outra"     # ex. surgir -> ressurgir

flag *E:
    .       >      PRI\-     # "FSEM=pre"       # ex. escolar -> pri-escolar

flag *O:
    .       >      PSS\-     # "FSEM=pos"       # ex. guerra -> p'os-guerra

flag *U:
    .       >      SUB\-      # "FSEM=sub"       # ex. problema -> sub-problema

suffixes

#-----------------------------------------
# plurais - os mais habituais
flag *p:         # "CAT=a_nc,N=s"
[^C][^LSMRNZX]>    S    # "N=p"         # -> s         ex. pato, patos
C E        >       S    # "N=p"         # ce -> ces    ex. mce, mces
C O        >  -CO,UES # "N=p"         # co -> ues    ex. leco, leues
A L         >    -L,IS   # "N=p"         # al -> ais    ex. animal, animais
O L         >   -OL,SIS # "N=p"         # ol -> sis    ex. anzol, anzsis
[^V] E L    >   -EL,IIS # "N=p"         # el -> iis    ex. papel, papiis
[AIMSZ] V E L > -L,IS # "N=p"       # el -> eis    ex. afavel, afaveis
I L         >    -L,S    # "N=p"         # il -> is     ex. funil, funis
[^AIJ] S >       ES   # "N=p"         # -> es        ex. deus, deuses
A S        >  -AS,ASES # "N=p"         # as -> ases   ex. gas, gases
I S        >  -IS,ESES # "N=p"         # is -> eses   ex. revis,reveses
J S        >  -JS,ESES # "N=p"         # js -> eses   ex. japonjs,japoneses
M           >    -M,NS   # "N=p"         # m -> ns      ex. jovem, jovens
[RNZX]      >       ES   # "N=p"         # -> es        ex. cantor, cantores

flag *a:         # "CAT=a_nc,N=s"
C O        >       S    # "N=p"         # co -> cos     ex. mco, mcos
I L         >   -IL,EIS  # "N=p"         # il -> eis     ex. agil, ageis  # deve ser posto em todos os que tjm acento e acabam em il

flag *e:         # "CAT=a_nc,N=s"
C O        >    -O,ES   # "N=p"         # co -> ces     ex. pco, pces

#-----------------------------------------
# femininos - os mais habituais
